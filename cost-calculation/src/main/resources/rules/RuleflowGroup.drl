
package rules

rule "start ruleflow distribution"
    salience -500
	when
	then
		kcontext.getKieRuntime().getAgenda().getAgendaGroup("distribution").setFocus();
end
rule "start ruleflow calculation"
    salience -500
    ruleflow-group "distribution"
	when
	then
		kcontext.getKieRuntime().getAgenda().getAgendaGroup("calculate").setFocus();
end
rule "start ruleflow total"
    salience -500
    ruleflow-group "calculate"
	when
	then
		kcontext.getKieRuntime().getAgenda().getAgendaGroup("total").setFocus();
end
